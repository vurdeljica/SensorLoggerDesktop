{
  "name": "sensorlogger",
  "version": "1.0.0",
  "description": "Desktop version of SensorLogger",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "generatePBS": "node ./node_modules/protobufjs/cli/bin/pbjs -t static-module -w commonjs -o sensordata.js proto/schema.proto --keep-case",
    "makeExe": "electron-packager ./ sensorLogger --platform=win32 --arch=x64"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vurdeljica/SensorLoggerDesktop.git"
  },
  "author": "Igor Vurdelja",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vurdeljica/SensorLoggerDesktop/issues"
  },
  "homepage": "https://github.com/vurdeljica/SensorLoggerDesktop#readme",
  "dependencies": {
    "address": "^1.1.1",
    "better-sqlite3": "^5.4.2",
    "bonjour": "^3.5.0",
    "bonjour-hap": "^3.5.4",
    "chart.js": "^2.8.0",
    "chartjs-plugin-zoom": "^0.7.3",
    "dygraphs": "^2.1.0",
    "electron": "^5.0.6",
    "electron-packager": "^14.0.5",
    "file-type": "^12.2.0",
    "formidable": "^1.2.1",
    "fstorm": "^0.1.3",
    "highcharts": "^7.1.3",
    "node-notifier": "^5.4.1",
    "portfinder": "^1.0.21",
    "protobufjs": "^6.8.8",
    "read-chunk": "^3.2.0",
    "sqlite3": "^4.0.9",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "electron-builder": "^21.2.0",
    "electron-rebuild": "^1.8.5"
  }
}
